{
  "short_name": "Nervos",
  "name": "Nervos App",
  "start_url": "./index.html",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "browser_action": {
    "default_popup": "./index.html",
    "default_title": "Nervos"
  },
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
